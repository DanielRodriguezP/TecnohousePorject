
@{
    ViewBag.Title = "Index";
}
<div class="col-md-12">
    <div class="card-header col-md-12 row">
        <div class="col-md-6">
            <h5>Consolidado</h5>
        </div>
    </div>
    <div class="card-body" style="margin-left: -2%; margin-right: -2%;">
        <div class="col-md-12  row" style="margin-top: 1%;">
            <div class="col-md-6">
                <label>Fecha Inicio <span style="color:red">(*)</span></label>
                <input class="form-control" id="FechaInicio" />
            </div>
            <div class="col-md-6">
                <label>Fecha Final  <span style="color:red">(*)</span></label>
                <input class="form-control" id="FechaFin" />
            </div>
        </div>
        <br />
        <div class="col-md-12 form-group">
            <center>
                <button class="btn btn-info" id="BtnGenerarExcel"> <h6><span class="fa fa-search"></span> Generar Excel</h6></button>
            </center>
        </div>
        <div class="col-md-12 row overflow-auto">
            <div id="gridLista">

            </div>
        </div>
        <div class="card-footer small text-muted"></div>
    </div>
</div>
<div id="ModalCarga" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document" style="vertical-align:middle">
        <div class="modal-content">
            <div class="modal-body">
                <img style="display: block; margin: auto;" src="~/Content/Images/carga.gif" />
            </div>
            <div class="modal-footer">
                <h1 id="hcarga" style="text-align:center;"></h1>
            </div>
        </div>
    </div>
</div>


    <script src="~/Scripts/Views/Consolidado/IndexConsolidado.js"></script>


