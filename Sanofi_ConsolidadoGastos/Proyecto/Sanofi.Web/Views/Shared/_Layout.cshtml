<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    <link href="~/Content/jquery-ui.min.css" rel="stylesheet" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap/bootstrap-datepicker.min.css" rel="stylesheet" />
    <link href="~/Content/font-awesome.min.css" rel="stylesheet" />
    <link href="~/Content/kendo/2017.1.118/kendo.common-bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/kendo/2017.1.118/kendo.common-material.min.css" rel="stylesheet">
    <link href="~/Content/kendo/2017.1.118/kendo.material.min.css" rel="stylesheet">
    <link href="~/Content/sb-admin.css" rel="stylesheet" />
    <link href="~/Content/sweetalert/sweet-alert.css" rel="stylesheet" />
    <link rel="shortcut icon" type="image/jpg" href="~/Content/Images/Sanofi-Logo.jpg">

    <script src="~/Scripts/Librerias/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/kendo/2017.1.118/kendo.all.min.js"></script>
    <script src="~/Scripts/Librerias/bootstrap.min.js"></script>
    <script src="~/Scripts/kendo/2017.1.118/jszip.min.js"></script>
    <script src="~/Scripts/Librerias/modernizr-2.6.2.js"></script>

    <!-- Custom scripts for all pages-->
    <script src="~/Scripts/Librerias/sweetalert.min.js"></script>
    <script src="~/Scripts/moment.js"></script>
    <script src="~/Scripts/moment.min.js"></script>
    <script src="~/Scripts/Librerias/Mensajes.js"></script>
    <script src="~/Scripts/VerificacionRuta.js"></script>
    @*<script src="~/Scripts/Views/Layout/Layout.js"></script>*@
</head>
<body id="page-top">

    <nav class="navbar navbar-expand navbar-dark bg-dark static-top">

        <a class="navbar-brand mr-1" href="#">Sanofi</a>

        <button class="btn btn-link btn-sm text-white order-1 order-sm-0" id="sidebarToggle" href="#">
            <i class="fa fa-bars"></i>
        </button>

        <!-- Navbar Search -->
        <form class="d-none d-md-inline-block form-inline ml-auto mr-0 mr-md-3 my-2 my-md-0"></form>

        <!-- Navbar -->
        <ul class="navbar-nav ml-auto ml-md-0">
            <li class="nav-item dropdown no-arrow">

            </li>
        </ul>
    </nav>
    <div id="wrapper">
        <!-- Sidebar -->
        <ul class="sidebar navbar-nav">
            <li class="nav-item dropdown" id="Athena">
                <a class="nav-link dropdown-toggle" href="#" id="pagesDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                    @*<i class="fa fa-user-md"></i>*@
                    <span>Athena</span>
                </a>
                <div class="dropdown-menu" aria-labelledby="pagesDropdown">
                    <a class="dropdown-item" href="#" id="IndexAthena" onClick="$('#IndexAthena').prop('href', sessionStorage.getItem('RutaActual')+'Athena/IndexAthena');">Importar Athena</a>
                    <a class="dropdown-item" href="#" id="ConsultaAthena" onClick="$('#ConsultaAthena').prop('href', sessionStorage.getItem('RutaActual')+'Athena/ConsultaAthena');">Consultar Athena</a>
                    @*<a class="dropdown-item" href="#" id="ReporteAthena" onClick="$('#ReporteAthena').prop('href', sessionStorage.getItem('RutaActual')+'Athena/ReporteAthena');">Reporte Athena</a>*@
                </div>
            </li>
            <li class="nav-item dropdown" id="Solped">
                <a class="nav-link dropdown-toggle" href="#" id="pagesDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                    @*<i class="fa fa-user-md"></i>*@
                    <span>Solped</span>
                </a>
                <div class="dropdown-menu" aria-labelledby="pagesDropdown">
                    <a class="dropdown-item" href="#" id="IndexSolped" onClick="$('#IndexSolped').prop('href', sessionStorage.getItem('RutaActual')+'Solped/IndexSolped');">Importar Solped</a>
                    <a class="dropdown-item" href="#" id="ConsultaSolped" onClick="$('#ConsultaSolped').prop('href', sessionStorage.getItem('RutaActual')+'Solped/ConsultaSolped');">Consultar Solped</a>
                </div>
            </li>
            <li class="nav-item dropdown" id="Gastos">
                <a class="nav-link dropdown-toggle" href="#" id="pagesDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                    @*<i class="fa fa-user-md"></i>*@
                    <span>Gastos</span>
                </a>
                <div class="dropdown-menu" aria-labelledby="pagesDropdown">
                    <a class="dropdown-item" href="#" id="IndexGastos" onClick="$('#IndexGastos').prop('href', sessionStorage.getItem('RutaActual')+'Gastos/IndexGastos');">Importar Gastos</a>
                    <a class="dropdown-item" href="#" id="ConsultaGastos" onClick="$('#ConsultaGastos').prop('href', sessionStorage.getItem('RutaActual')+'Gastos/ConsultaGastos');">Consultar Gastos</a>
                    @*<a class="dropdown-item" href="#" id="ReporteGastos" onClick="$('#ReporteGastos').prop('href', sessionStorage.getItem('RutaActual')+'Gastos/ReporteGastos');">Reporte Gastos</a>*@
                </div>
            </li>
            <li class="nav-item dropdown" id="Consolidado">
                <a class="nav-link dropdown-toggle" href="#" id="pagesDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                    <span>Consolidado</span>
                </a>
                <div class="dropdown-menu" aria-labelledby="pagesDropdown">
                    <a class="dropdown-item" href="#" id="IndexConsolidado" onClick="$('#IndexConsolidado').prop('href', sessionStorage.getItem('RutaActual')+'Consolidado/IndexConsolidado');">Exportar excel</a>
                </div>
            </li>
        </ul>
        <div id="content-wrapper">
            <div class="container-fluid">
                @RenderBody()
            </div>
            <!-- /.container-fluid -->
            <!-- Sticky Footer -->
            <footer class="sticky-footer">
                <div class="container my-auto">
                    <div class="copyright text-center my-auto">
                        <p>Todos los derechos reservados - Algartech © @DateTime.Now.Year <br>Diseño y Desarrollo AlgarTech</p>
                    </div>
                </div>
                <div style="font-size:small;margin-top: 3%;width:10%">
                    <span id="Version"></span>
                </div>
            </footer>
        </div>
        <!-- /.content-wrapper -->
    </div>
    <!-- /#wrapper -->
    <!-- Scroll to Top Button-->
    <a class="scroll-to-top rounded" href="#page-top">
        <i class="fa fa-angle-up"></i>
    </a>
</body>
</html>
@*<script src="~/Scripts/Librerias/jquery-1.10.2.js"></script>*@

<script src="~/Scripts/Librerias/sb-admin.min.js"></script>
